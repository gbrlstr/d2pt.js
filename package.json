{
  "name": "d2pt.js",
  "version": "2.0.0",
  "description": "Scraper Node.js para Dota 2 Pro Tracker — meta de heróis e informações por herói",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    }
  },
  "types": "./lib/index.d.ts",
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "node -e \"require('fs').rmSync('lib', {recursive: true, force: true})\"",
    "compile": "tsup ./src/index.ts --format cjs,esm --dts --outDir ./lib",
    "test": "jest",
    "example:hero": "npx ts-node examples/example_hero.ts",
    "example:meta": "npx ts-node examples/example_meta_hero.ts",
    "watch": "npm run build -- --watch",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.mjs",
    "lib/**/*.d.ts",
    "lib/**/*.d.mts"
  ],
  "keywords": [
    "dota2protracker",
    "dota2-pro-tracker",
    "stratz",
    "dota",
    "dota2",
    "scraper",
    "meta",
    "heroes"
  ],
  "author": "GBRL DEV (https://github.com/gbrlstr)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gbrlstr/d2pt.js/issues"
  },
  "homepage": "https://github.com/gbrlstr/d2pt.js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbrlstr/d2pt.js.git"
  },
  "dependencies": {
    "puppeteer": "^22.15.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "puppeteer-select": "^1.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^20.14.5",
    "dotenv": "^16.4.5",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
